/* 1150 - Exceeding Z */

#include<stdio.h>
int main()
{
    int x,z,sum=0,s=0;
    scanf("%d",&x);
    while(1)
    {
        scanf("%d",&z);
        if(x<z)
        {

            break;
        }
    }
    for(int i=x; i<z; i++)
    {
        sum+=i;
        s+=1;
        if(sum>z)
        {
            break;
        }
    }
    printf("%d\n",s);


    return 0;
}
