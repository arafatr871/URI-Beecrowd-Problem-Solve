/* Sum of Consecutive Even Numbers */

#include<stdio.h>
int main()
{
    int n,x,y,c,sum;

    while(1)
    {
        scanf("%d",&x);
        sum=0;
        c=0;
        if(x==0)
        {
            break;
        }
        if(x%2!=0)
        {
            x++;
        }
        while(c<5)
        {
            sum+=x;
            x+=2;
            c++;

        }

        printf("%d\n",sum);
    }




    return 0;

}
