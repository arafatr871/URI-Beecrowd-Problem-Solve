/* 1789 - The Race of Slugs */

#include<stdio.h>
int main()
{
    int n,lv=0,a;

    while(scanf("%d",&n)!=EOF)
    {
        int j=0;
        for(int i=0; i<n; i++)
        {
            scanf("%d",&a);
            if(j<a)
            {
                j=a;
            }
        }

            if(j<10)
            {
                lv=1;
            }
            else if(j>=10 && j<20)
            {
                lv=2;
            }
            else if(j>=20)
            {
                lv=3;
            }

        printf("%d\n",lv);
    }
    return 0;
}
