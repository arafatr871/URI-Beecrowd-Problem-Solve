/* 1866 - Bill */

#include<stdio.h>

int main()
{
    int n,k,h;

    scanf("%d",&n);

    for(int i=0;i<n;i++)
    {
        scanf("%d",&k);
        h=1;
        for(int j=1;j<k;j++)
        {
            if(j%2==0)
            {
                h=h+1;
            }
            else{
                h=h-1;
            }
        }
        printf("%d\n",h);


    }

    return 0;
}
