/* 1847 - Welcome to the Winter! */

#include<stdio.h>
int main()
{
   int a,b,c,d,e;
   scanf("%d %d %d",&a,&b,&c);
   d=b-a;
   e=c-b;
   if(d<0 && e>=0)//1
   {
       printf(":)\n");
   }
   else if(d>0 && e>0 && d<=e)//4
   {
        printf(":)\n");
   }
   else if(d>0 && e<=0)//2
   {
        printf(":(\n");
   }
   else if(d>0 && e>0 && e<d)//3
   {
        printf(":(\n");
   }
   else if(d<0 && e<0 && (b-c)<(a-b))//5
   {
        printf(":)\n");
   }
   else if(d<0 && e<0 && (b-c)>=(a-b))//6
   {
        printf(":(\n");
   }
   else if(d==0)
   {
       if(e>0)
       {
           printf(":)\n");
       }
       else
       {
          printf(":(\n");
       }
   }


   return 0;
}
