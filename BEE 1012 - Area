/* Area */

#include<stdio.h>
#include<math.h>
int main()
{
  double A,B,pi=3.14159,C,tri,cir,trp,squ,rec;
  scanf("%lf %lf %lf",&A,&B,&C);
  tri=(1/2.0)*A*C;
  cir=pi*C*C;
  trp=(1/2.0)*(A+B)*C;
  squ=B*B;
  rec=A*B;
  printf("TRIANGULO: %.3lf\nCIRCULO: %.3lf\nTRAPEZIO: %.3lf\nQUADRADO: %.3lf\nRETANGULO: %.3lf\n",tri,cir,trp,squ,rec);
  return 0;
}
