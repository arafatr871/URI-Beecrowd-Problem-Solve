/* Above the Main Diagonal */

#include<stdio.h>
int main()
{
    double m[12][12],sum=0.0;
    
    char A[2];
    scanf("%s",&A);

    for(int i=0; i<12; i++)
    {
        for(int j=0; j<12; j++)
        {
            scanf("%lf",&m[i][j]);
        }
    }
    for(int i=0; i<12; i++)
    {
        for(int j=0; j<12; j++)
        {
            if(j>i)
            {
                sum+= m[i][j];
            }
        }
    }
    
    if(A[0]=='S')
    {
        printf("%.1lf\n",sum);
    }
    else if(A[0]=='M')
    {
        printf("%.1lf\n",sum/66.0);
    }


    return 0;
}
