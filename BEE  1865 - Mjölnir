/* 1865 - Mj√∂lnir */

#include<stdio.h>
#include<string.h>
int main()
{
    int n,k;
    char arr[20];
    scanf("%d",&n);

    for(int i=0;i<n;i++)
    {
        scanf("%s %d",&arr,&k);
        if(strcmp(arr,"Thor")==0)
        {
            printf("Y\n");
        }
        else
        {
            printf("N\n");
        }

    }

    return 0;
}
