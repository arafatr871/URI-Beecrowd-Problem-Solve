/* Interval 2 */

#include<stdio.h>
int main()
{
    int n,x,sum=0,s=0;

    scanf("%d",&n);
    for(int i=1; i<=n; i++)
    {
        scanf("%d",&x);
        if(x>=10 && x<=20)
        {
            sum++;
        }
        else
        {
            s++;
        }
    }
    printf("%d in\n%d out\n",sum,s);
    return 0;
}

